places_to_visit = ["Portugal", "Thailand", "Japan", "Brazil", "Canada"]

print(places_to_visit)

print("n\Here is the sorted list: ")
print(sorted(places_to_visit))

places_to_visit.sort()
print(places_to_visit)
print(len(places_to_visit))

numbers = [1, 3, 57, 4, 9, 84, 67, 13, 21]

print(sum(numbers))
print(len(numbers))
numbers.sort()
print(numbers)

# Using .sort() will edit the list whereas sorted will leave the original list unchanged but show you the results
